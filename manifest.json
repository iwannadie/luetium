{
  "manifest_version": 2,
  "name": "luetium",
  "description": "dont exist foo",
  "version": "0.1.0",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "clipboardWrite",
    "notifications",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "src/util/notify.js",
      "src/util/ajax.js",
      "src/background/transloader.js"
    ]
  },
  "content_scripts": [
    { "matches": [
        "http://boards.endoftheinter.net/showmessages.php*",
        "https://boards.endoftheinter.net/showmessages.php*",
        "http://endoftheinter.net/inboxthread.php*",
        "https://endoftheinter.net/inboxthread.php*"
      ],
      "js": [
        "third_party/favico-0.3.5.min.js",
        "src/util/dom.js",
        "src/util/strings.js",
        "src/content/favicon.js",
        "src/content/myuser.js",
        "src/content/messages/post.js",
        "src/content/messages/livelinks.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "src/content/messages/navi.js"
  ]
}
